{
  "extends": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["prettier", "prefer-arrow", "@typescript-eslint"],
  "rules": {
    "prettier/prettier": ["error", {
      "printWidth": 80,
      "singleQuote": true,
      "semi": false,
      "quoteProps": "consistent"
    }],
    "prefer-arrow/prefer-arrow-functions": ["error", {} ],
    "no-console": ["error", {}]
  }
}